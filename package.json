{
  "name": "ws-promise",
  "version": "1.5.8",
  "description": "A tiny, Promise-based WebSocket protocol allowing request-response usage in ECMAScript",
  "main": "./index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "check-commit": "npm test && npm run lint",
    "compile": "babel src -d .",
    "coverage": "nyc npm test",
    "lint": "eslint src",
    "test": "ava",
    "prepublishOnly": "npm run check-commit && npm run compile"
  },
  "keywords": [
    "ws",
    "websocket",
    "promise",
    "client",
    "protocol",
    "subprotocol",
    "esnext",
    "es2015",
    "es2016",
    "es2017",
    "es6",
    "modern"
  ],
  "author": "kdex",
  "license": "GPL-3.0",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-filenames": "^1.2.0",
    "greenkeeper-lockfile": "^1.14.0",
    "nyc": "^11.7.1"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit",
    "files": [
      "test/**/*.test.js"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "crystal-event-emitter": "^2.1.5",
    "msgpack-lite": "^0.1.26",
    "uuid": "^3.2.1",
    "ws": "^5.1.1"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdex/ws-promise.git"
  },
  "bugs": {
    "url": "https://github.com/kdex/ws-promise/issues"
  },
  "homepage": "https://github.com/kdex/ws-promise#readme"
}
